<div class="booyah-box col-10 offset-1">
  <h1>Enter Your Playground Info</h1>
   <%= simple_form_for @location do |f| %>
     <%= f.input :name %>
     <%= f.input :description %>
     <%= f.input :address %>
     <br />
     <%= f.submit 'Create', class: 'btn btn-primary' %>
   <% end %>
</div>